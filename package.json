{
  "name": "handyman-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "cd . && python main.py",
    "dev:full": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "validate:env": "node scripts/validate-env.js",
    "setup:dev": "node scripts/setup-dev.js",
    "test:integration": "node tests/integration-test.js",
    "test:admin": "python tests/test-admin.py",
    "test:all": "npm run test:integration && npm run test:admin",
    "upgrade:deps": "npm update && pip install -r requirements.txt --upgrade",
    "health:check": "curl -f http://localhost:5173/health || exit 1",
    "deploy:check": "npm run build && npm run test:all",
    "security:audit": "npm audit && pip-audit -r requirements.txt"
  },
